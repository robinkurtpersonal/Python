#name & description
print("Name: Basic_Program_Power ")
print("Description:Sets one integer as the base and the other as the exponent.")
print("")

#basic_program 
integer_one = int(input("Enter the first integer (base): "))
integer_two = int(input("Enter the second integer (Exponent): "))

power = integer_one ** integer_two

print(f"The power of the 2 integers (Base, exponent) is {power}.")
#end of basic_program


#signature & trademark
print("")
